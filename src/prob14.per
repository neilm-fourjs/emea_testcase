l_to = FORMONLY.m_timeout;
l_rwto = FORMONLY.m_rw_timeout;
TEXTEDIT m_cmd = FORMONLY.m_cmd, STRETCH=BOTH, NOENTRY;
TEXTEDIT l_msg = FORMONLY.m_msg, STRETCH=BOTH, NOENTRY;
X = FORMONLY.x, NOENTRY;